{
  "manifest_version": 3,
  "name": "Auto Fill Blanks Debug",
  "version": "1.0",
  "description": "Automatically fills in blank answers with debugging",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://www.doddsre.com/*"],
    "js": ["content.js"]
  }]
}