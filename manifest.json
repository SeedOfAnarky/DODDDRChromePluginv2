{
  "manifest_version": 3,
  "name": "Dodd's Helper",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*.doddsre.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
"content_scripts": [{
    "matches": ["*://*.doddsre.com/*"],
    "js": ["content.js"]
}]
}